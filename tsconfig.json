{
  "extends": "astro/tsconfigs/strict",
  "include": ["."],
  "compilerOptions": {
    "allowJs": false,
    "baseUrl": ".",
    "exactOptionalPropertyTypes": true,
    "jsx": "react-jsx",
    "lib": ["DOM", "ESNext"],
    "paths": {
      "~/*": ["./src/*"]
    },
    "noUncheckedIndexedAccess": true
  }
}
