{
  "private": true,
  "name": "@inolib/website-backend",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "build": "astro build",
    "format": "pnpm run format:prettier && pnpm run format:jsxattribute-sort && pnpm run format:packagejson && pnpm run format:sort-imports && pnpm run format:tailwindcss",
    "format:jsxattribute-sort": "prettier --config ./.config/prettier/jsxattribute-sort/prettier.config.js --write ./**/*.tsx",
    "format:packagejson": "prettier --config ./.config/prettier/packagejson/prettier.config.js --write ./package.json",
    "format:prettier": "prettier --write .",
    "format:sort-imports": "prettier --config ./.config/prettier/sort-imports/prettier.config.js --write ./**/*.js ./**/*.ts ./**/*.tsx",
    "format:tailwindcss": "prettier --config ./.config/prettier/tailwindcss/prettier.config.js --write ./**/*.astro ./**/*.ts ./**/*.tsx",
    "knip": "knip",
    "lint": "pnpm run lint:astro && pnpm run lint:eslint && pnpm run lint:tsc",
    "lint:astro": "astro check",
    "lint:eslint": "eslint --ext .cjs,.js,.ts,.tsx .",
    "lint:tsc": "tsc",
    "serve:dev": "astro dev --host",
    "serve:preview": "astro preview",
    "test": "vitest"
  },
  "dependencies": {
    "@apollo/client": "^3.8.7",
    "@fontsource/atkinson-hyperlegible": "^5.0.17",
    "@nanostores/persistent": "^0.9.1",
    "@nanostores/react": "^0.7.1",
    "clsx": "^2.0.0",
    "graphql": "^16.8.1",
    "immer": "^10.0.3",
    "nanoid": "^5.0.3",
    "nanostores": "^0.9.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-modal": "^3.16.1",
    "tailwind-merge": "^2.0.0",
    "use-immer": "^0.9.0"
  },
  "devDependencies": {
    "@astrojs/react": "^3.0.6",
    "@astrojs/tailwind": "^5.0.2",
    "@ianvs/prettier-plugin-sort-imports": "^4.1.1",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.39",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "astro": "^3.6.1",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "happy-dom": "^12.10.3",
    "knip": "^3.6.0",
    "postcss": "^8.4.31",
    "prettier": "^3.1.0",
    "prettier-plugin-jsxattribute-sort": "^1.0.1",
    "prettier-plugin-packagejson": "^2.4.6",
    "prettier-plugin-tailwindcss": "^0.5.7",
    "tailwindcss": "^3.3.5",
    "typescript": "^5.3.2",
    "vitest": "^0.34.6"
  }
}
